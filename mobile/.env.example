# ACHIEVEMOR Mobile App Environment Configuration
# Copy this file to .env and configure your environment variables

# API Configuration
EXPO_PUBLIC_API_URL=https://your-app.replit.app
EXPO_PUBLIC_ENVIRONMENT=development

# Stripe Configuration (for payments)
EXPO_PUBLIC_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here

# Google Maps API (for location services)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=ACHIEVEMOR
EXPO_PUBLIC_APP_VERSION=1.0.0

# Push Notifications (optional)
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your_expo_push_key_here

# Analytics (optional)
EXPO_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Debug Settings
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=info